{
  "name": "page-speed",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "eslint . --fix",
    "postinstall": "nuxt prepare && simple-git-hooks",
    "test:e2e": "playwright test test/e2e",
    "test:e2e:update": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.48.0-focal bash -c 'corepack enable && pnpm i && pnpm playwright test test/e2e --update-snapshots'"
  },
  "dependencies": {
    "@azure/identity": "^4.4.1",
    "@azure/storage-blob": "^12.25.0",
    "@neoconfetti/vue": "^2.2.1",
    "@nuxtjs/html-validator": "^1.8.2",
    "@nuxtjs/plausible": "^1.0.3",
    "@unocss/nuxt": "^0.63.4",
    "nitropack": "^2.9.7",
    "nuxt": "^3.13.2",
    "nuxt-og-image": "^3.0.4",
    "nuxt-time": "^1.0.2",
    "ufo": "^1.5.4",
    "vue": "^3.5.11"
  },
  "devDependencies": {
    "@nuxt/eslint": "0.5.7",
    "@nuxt/test-utils": "^3.14.3",
    "@playwright/test": "^1.48.0",
    "eslint": "9.12.0",
    "lint-staged": "15.2.10",
    "mock-driver": "link:./test/mock-driver",
    "simple-git-hooks": "2.11.1",
    "typescript": "5.6.3",
    "unstorage": "^1.12.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs,json,.*rc}": [
      "npx eslint --fix"
    ]
  },
  "packageManager": "pnpm@9.12.1"
}
